      <inertia ixx="0.0020291666666666665" ixy="0.0" ixz="0.0" iyy="0.0020291666666666665" iyz="0.0" izz="2.5e-05"/>
    </inertial>
  </link>  

  <!-- Standoff Rod Front Right -->
